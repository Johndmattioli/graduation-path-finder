@model GPF.Web.ViewModels.DegreeListViewModel
@{
    ViewBag.Title = "Degrees";
}

<div class="page-header">
    <h1>@ViewBag.Title</h1>
</div>

<div class="panel-group">
    @foreach (var degree in Model.Degrees)
    {
        <div class="panel panel-default">
            <div class="panel-heading">
                <h4 class="panel-title">
                    <a data-toggle="collapse" href="#degree-@degree.Id">@degree.Title</a>
                </h4>
            </div>
            <div id="degree-@degree.Id" class="panel-collapse collapse">
                <div class="panel-body">Panel Body</div>
            </div>
        </div>
    }
</div>
